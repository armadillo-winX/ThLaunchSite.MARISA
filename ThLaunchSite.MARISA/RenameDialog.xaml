<Window x:Class="ThLaunchSite.MARISA.RenameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThLaunchSite.MARISA"
        mc:Ignorable="d"
        Title="リプレイファイルのリネーム" Height="160" Width="326" ShowInTaskbar="False" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid>
        <TextBox
            x:Name="ReplayNameBox"
            Margin="10,43,35,0" TextWrapping="NoWrap" Text="" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Right" TextWrapping="Wrap" Text=".rpy" VerticalAlignment="Top" Margin="0,45,10,0"/>
        <Button
            x:Name="OKButton"
            Width="90" Height="24"
            IsDefault="True"
            Content="_OK" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Click="OKButtonClick"/>

    </Grid>
</Window>
